{
  "name": "fun-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "boot:ui": "npm run serve",
    "cypress": "cypress open",
    "test": "cypress run",
    "test:visual": "npm run cypress",
    "cypress:init": "node_modules/.bin/cypress open",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:prod": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "build_test": "tsc",
    "killdist": "rm -rf dist",
    "m": "npx ts-node $1",
    "cycle": "npm run build && cp -r dist/* $npm_package_config_kitpath/src/frontend/dist/ && cd $npm_package_config_kitpath && npm run deploy:frontend:full && echo $PWD && cd deploy/frontend && echo 'done'"
  },
  "config": {
    "kitpath": "../__dfcheatkit/"
  },
  "dependencies": {
    "df-cheatcodes-base": "^0.1.7",
    "chart.xkcd": "^1.1.13",
    "chart.xkcd-vue": "^1.1.0",
    "core-js": "^3.6.4",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
    "nes.css": "^2.3.0",
    "showdown": "^1.9.1",
    "typeface-press-start-2p": "0.0.72",
    "vodal": "^2.4.0",
    "vue": "^2.6.11",
    "vue-carousel-3d": "^0.2.0"
  },
  "devDependencies": {
    "@types/tape": "^4.13.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^4.9.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "tape": "^5.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
